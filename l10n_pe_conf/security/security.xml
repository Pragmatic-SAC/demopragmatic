<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="pragmatic_category" model="ir.module.category">
        <field name="name">Pragmatic Localization</field>
        <field name="description">
            Category for the localizations
        </field>
        <field name="sequence">5</field>
    </record>
    <record id="pragmatic_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="pragmatic_category"/>
    </record>
    <record id="pragmatic_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="pragmatic_category"/>
    </record>
</odoo>